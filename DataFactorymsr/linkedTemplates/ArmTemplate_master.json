{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "DataFactorymsr"
		},
		"ls_ablob_reporting_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_reporting'"
		},
		"ls_adls_reporting_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_reporting'"
		},
		"AzureSqlDatabase_Happy_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDatabase_Happy'"
		},
		"ls_adls_reporting_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storagedlmsr.dfs.core.windows.net/"
		},
		"ls_http_opendata_ecdc_europe_eu_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://github.com/"
		},
		"tr_ingest_hospital_admissions_data_properties_pls_ingest_ecdc_data_parameters_sourceRelativeurl": {
			"type": "string",
			"defaultValue": "cloudboxacademy/covid19/blob/main/raw/main/ecdc_data/hospital_admissions.csv"
		},
		"tr_ingest_hospital_admissions_data_properties_pls_ingest_ecdc_data_parameters_sinkFileName": {
			"type": "string",
			"defaultValue": "hospital_admissions.csv"
		},
		"tr_populationData_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/e418e5e7-fa19-48bd-95c2-fffb8c0f2d20/resourceGroups/dp203-8q21z67/providers/Microsoft.Storage/storageAccounts/storagenormalmsr"
		},
		"AzureSqlDatabase_Happy_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "happyservermsr.database.windows.net"
		},
		"AzureSqlDatabase_Happy_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "Happy"
		},
		"AzureSqlDatabase_Happy_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "ms2901"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "DataFactorymsr_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_ablob_reporting_connectionString": {
						"value": "[parameters('ls_ablob_reporting_connectionString')]"
					},
					"ls_adls_reporting_accountKey": {
						"value": "[parameters('ls_adls_reporting_accountKey')]"
					},
					"AzureSqlDatabase_Happy_password": {
						"value": "[parameters('AzureSqlDatabase_Happy_password')]"
					},
					"ls_adls_reporting_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_reporting_properties_typeProperties_url')]"
					},
					"ls_http_opendata_ecdc_europe_eu_properties_typeProperties_url": {
						"value": "[parameters('ls_http_opendata_ecdc_europe_eu_properties_typeProperties_url')]"
					},
					"AzureSqlDatabase_Happy_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabase_Happy_properties_typeProperties_server')]"
					},
					"AzureSqlDatabase_Happy_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabase_Happy_properties_typeProperties_database')]"
					},
					"AzureSqlDatabase_Happy_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabase_Happy_properties_typeProperties_userName')]"
					},
					"tr_ingest_hospital_admissions_data_properties_pls_ingest_ecdc_data_parameters_sourceRelativeurl": {
						"value": "[parameters('tr_ingest_hospital_admissions_data_properties_pls_ingest_ecdc_data_parameters_sourceRelativeurl')]"
					},
					"tr_ingest_hospital_admissions_data_properties_pls_ingest_ecdc_data_parameters_sinkFileName": {
						"value": "[parameters('tr_ingest_hospital_admissions_data_properties_pls_ingest_ecdc_data_parameters_sinkFileName')]"
					},
					"tr_populationData_properties_typeProperties_scope": {
						"value": "[parameters('tr_populationData_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}